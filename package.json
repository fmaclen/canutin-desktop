{
	"name": "canutin",
	"version": "0.0.0-dev",
	"private": true,
	"scripts": {
		"dev": "concurrently \"vite dev\" \"typesafe-i18n\" \"npm run pb\"",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"pb": "./pocketbase/pocketbase serve",
		"pb:typegen": "pocketbase-typegen --db ./pocketbase/pb_data/data.db -o ./src/lib/pocketbase-types.ts",
		"pb:seed": "vite-node ./src/lib/seed/index.ts",
		"pb:clean": "rm -rf ./pocketbase/pb_data",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "^4.3.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@tailwindcss/vite": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"@types/papaparse": "^5.3.14",
		"concurrently": "^8.2.2",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"papaparse": "^5.4.1",
		"pocketbase": "^0.21.5",
		"pocketbase-typegen": "^1.2.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.19.3",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^4.0.0",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vite-node": "^2.0.5"
	},
	"optionalDependencies": {
		"@cloudflare/workerd-linux-64": "1.20240903.0",
		"@rollup/rollup-linux-x64-gnu": "^4.21.2",
		"@sveltejs/adapter-cloudflare": "^4.7.2"
	},
	"type": "module"
}
